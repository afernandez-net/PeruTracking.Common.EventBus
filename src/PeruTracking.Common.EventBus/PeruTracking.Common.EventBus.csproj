<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>PeruTracking.Common.EventBus</Title>
    <Authors>Luis Alexander Fern√°ndez</Authors>
    <Company>Personal</Company>
    <RepositoryUrl>https://github.com/afernandez-net/PeruTracking.Common.EventBus</RepositoryUrl>
    <PackageReleaseNotes>Initial release</PackageReleaseNotes>
    <Description>Paquete de soporte para mensajeria asincrona con RabbitMQ y Event Sourcing con MediatR</Description>
